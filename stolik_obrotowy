Table user {
  id integer [primary key]
  user_id integer
  user_name string
  watch string
}

Table device {
  id integer [primary key]
  user_id integer 

  max_battery integer
  kolor string
}

Table data {
  id integer [primary key]
  device_id integer
aktualny_czas
  aktualne_naswietlenie integer
  czas_naswietlenia double
  kat_padania integer
  stan_baterii integer 
  status_ladowania bool
}
Ref: device.user_id < user.id 





Ref: "device"."id" < "data"."device_id"
